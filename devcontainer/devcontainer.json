{
  "name": "Flutter Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter"
      ]
    }
  },
  "remoteEnv": {
    "PATH": "/root/flutter/bin:/root/.pub-cache/bin:${PATH}"
  },
  "postCreateCommand": "flutter doctor"
}
